<Window x:Class="DictaSamples.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Select" Height="350" Width="525"
        xmlns:dicta="clr-namespace:DictaDotNet;assembly=DictaWPF"
        dicta:DictaProperty.Model="dicta/samples/select.dicta">
    <Grid>
        <ComboBox dicta:DictaProperty.In="text" Margin="24,24,373,268" Width="120" Height="24">
            <TextBlock></TextBlock>
            <TextBlock>apple</TextBlock>
            <TextBlock>grapefruit</TextBlock>
            <TextBlock>orange</TextBlock>
            <TextBlock>peach</TextBlock>
        </ComboBox>
        <TextBlock dicta:DictaProperty.Out="upperCase" Margin="24,62,344,234" VerticalAlignment="Center" Height="28" FontWeight="Bold"/>
    </Grid>
</Window>
