a = {1, 2, 3};
b;

// b depends on a, i
// c depends on a, i
// When invalid b or c needs to be evaluated, loop will be executed in its entirety
for (i = 0; i < 3; i++) {
    b[i] = a[i] + 1;
    c = 2*i;
}

// Will invalidate b, c
i = 10;

e = 4;

// Will be executed only once at parse time, since no dependency is created
e++;